{
    "name": "hkt/expressive-auraviewrenderer",
    "description": "Aura View integration for Expressive",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "expressive",
        "http",
        "middleware",
        "psr",
        "psr-7",
        "aura",
        "view"
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "aura/view": "~2.0",
        "aura/html": "~2.0",
        "zendframework/zend-expressive-helpers": "^1.1 || ^2.0",
        "zendframework/zend-expressive-template": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
      "psr-4": {
        "Hkt\\Expressive\\AuraView\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Hkt\\Expressive\\AuraView\\": "tests/"
      }
    },
    "scripts": {
        "check": [
            "@cs",
            "@cs-fix",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
